{
  "compilerOptions": {
    "outDir": "./dist/",
    "strict": true,
    "alwaysStrict": true,
    "sourceMap": true,
    "module": "ESNext",
    "target": "ES5",
    "allowJs": true,
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable",
      "ScriptHost"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}